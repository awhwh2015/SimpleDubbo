<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
		http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd">

    <dubbo:application name="ordercenter" owner="jiawei.gao,yanghao.chen,chong.yang,jiong.chen,wanghan.wang,bowen.wang,guocai.zhang,huayang,zhou" organization="tc"/>


    <dubbo:provider>
        <dubbo:parameter key="qloglevel" value="6" />
    </dubbo:provider>

    <dubbo:consumer>
        <dubbo:parameter key="timeout" value="3000" />
        <dubbo:parameter key="qloglevel" value="6" />
    </dubbo:consumer>

    <import resource="dubbo-provider.xml" />
    <!--<import resource="consumer.xml" />-->
</beans>